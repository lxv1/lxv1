\begin{lemma}\label{lem:y5} For any complex numbers $ a, b$, with $ b \neq
0$, 
\begin{align}\label{eq:y7}
&\frac{(1+a/b)}{(aq)_{\infty}}
\sum_{n=0}^{\infty} \frac{(-1)^n a^n
q^{n(n+1)/2}}{(q)_n(aq/b)_n}-\frac{a}{b}\frac{1}{(aq)_{\infty}}
\sum_{n=0}^{\infty} \frac{(-1)^n a^n q^{n(n+1)/2+n}}{(q)_n
(aq/b)_n}\notag \\=&\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}
q^{n(3n-1)/2}}{(q)_n(aq/b)_n(aq)_n}.
\end{align}
\end{lemma}

\begin{proof}
We need a limiting case of Watson's $q$-analogue of Whipple's theorem 
given in 
Andrews's paper \cite[eq.~(2.3)]{page},
\begin{equation} \label{eq:y8}
\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}
q^{n(3n+1)/2}}{(q)_n (aq/b)_{n} (aq)_n}=\frac{1}{(aq)_{\infty}}
\sum_{n=0}^{\infty} \frac{(-1)^n a^n q^{n(n+1)/2}}{(q)_n (aq/b)_n}.
\end{equation}
By replacing $b$ by $bq$ and multiplying both sides by $(1-a/b)$,
we obtain
\begin{equation*}
\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}(1-a q^n/b)
q^{n(3n-1)/2}}{(q)_n (aq/b)_{n} (aq)_n}=\frac{1}{(aq)_{\infty}}
\sum_{n=0}^{\infty} \frac{(-1)^n a^n (1-a q^n/b) q^{n(n+1)/2}}
{(q)_n (aq/b)_n},
\end{equation*}
or,
\begin{align}
&\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}
q^{n(3n-1)/2}}{(q)_n (aq/b)_{n}
(aq)_n}-\frac{a}{b}\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}
q^{n(3n+1)/2}}{(q)_n (aq/b)_{n} (aq)_n} \notag\\
=&\frac{1}{(aq)_{\infty}} \sum_{n=0}^{\infty} \frac{(-1)^n a^n
 q^{n(n+1)/2}} {(q)_n (aq/b)_n}-\frac{a}{b}\frac{1}{(aq)_{\infty}}
\sum_{n=0}^{\infty} \frac{(-1)^n a^n
 q^{n(n+1)/2+n}} {(q)_n (aq/b)_n}.\notag
\end{align}
Using (\ref{eq:y8}) above, we deduce \eqref{eq:y7}. 
%\begin{align}
%&\sum_{n=0}^{\infty} \frac{(-1)^n a^{2n} b^{-n}
%q^{n(3n-1)/2}}{(q)_n (aq/b)_{n} (aq)_n} \notag\\
%=&\frac{1+a/b}{(aq)_{\infty}} \sum_{n=0}^{\infty} \frac{(-1)^n a^n
% q^{n(n+1)/2}} {(q)_n (aq/b)_n}-\frac{a}{b}\frac{1}{(aq)_{\infty}}
%\sum_{n=0}^{\infty} \frac{(-1)^n a^n
% q^{n(n+1)/2+n}} {(q)_n (aq/b)_n}.\notag
%\end{align}
\end{proof}

To maintain the flow of the next proof, we place here the two applications of
Lemma \ref{lem:y5} which will be needed.

If we set $ a = \o $ and $ b = \o^2 $ in \eqref{eq:y7}, we find that, after
some simplification,
\begin{align}\label{applic1}
&-\df{\o}{(\o
 q)_{\i}}\left\{\sum_{n=0}^{\i}\df{(-\o)^nq^{n(n+1)/2}}{(q)_n(\o^2q)_n} +
\o \sum_{n=0}^{\i}\df{(-\o)^nq^{n(n+1)/2+n}}{(q)_n(\o^2q)_n}\right\}\notag \\
=& \sum_{n=0}^{\i}\df{(-1)^nq^{n(3n-1)/2}}{(q^3;q^3)_n} = (q;q^3)_{\i},
\end{align}
by letting $ N \to \i $ in \eqref{binomial1}, with $ q $ replaced by $ q^3 $
and $ z = q$. 

Next, if we set $ a = \o^2 $ and $ b = \o $ in \eqref{eq:y7}, we find 
that, after some simplification,
\begin{align}\label{applic2}
&-\df{\o^2}{(\o^2
 q)_{\i}}\left\{\sum_{n=0}^{\i}\df{(-\o^2)^nq^{n(n+1)/2}}{(q)_n(\o q)_n} +
\o \sum_{n=0}^{\i}\df{(-\o^2)^nq^{n(n+1)/2+n}}{(q)_n(\o q)_n}\right\}\notag \\
=& \sum_{n=0}^{\i}\df{(-1)^nq^{n(3n-1)/2}}{(q^3;q^3)_n} = (q;q^3)_{\i}.
\end{align}


